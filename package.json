{
  "name": "shopfloor-file-service",
  "version": "2.2.1",
  "description": "shopfloor.io File Service",
  "private": true,
  "license": "Proprietary",
  "scripts": {
    "migration:create": "ts-node --require module-alias/register ../node_modules/typeorm/cli.js migration:create -n $1",
    "migration:generate": "ts-node --require module-alias/register ../node_modules/typeorm/cli.js migration:generate -n $1",
    "migration:run": "ts-node --require module-alias/register ../node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node --require module-alias/register ../node_modules/typeorm/cli.js migration:revert",
    "start": "node --max-http-header-size=65536 -r module-alias/register build/file-backend/src/app.js",
    "start:develop": "NODE_ENV=staging LOG_LEVEL=debug npm run start -- -r source-map-support/register",
    "start:staging": "NODE_ENV=staging LOG_LEVEL=info npm run start",
    "start:production": "NODE_ENV=production npm run start",
    "test": "NODE_ENV=testing jest -c .jest-unit.json --watchAll",
    "test:debug": "NODE_ENV=testing node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -c .jest-unit.json --runInBand",
    "test:once": "NODE_ENV=testing jest -c .jest-unit.json --coverage",
    "posttest:once": "npm run check",
    "e2e": "NODE_ENV=e2e jest -c .jest-e2e.json --watchAll",
    "e2e:debug": "NODE_ENV=e2e node --inspect-brk=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -c .jest-e2e.json --runInBand",
    "e2e:once": "NODE_ENV=e2e jest -c .jest-e2e.json",
    "check": "npm run check:lint && npm run check:format",
    "check:lint": "eslint -c .eslintrc.fix.json '{src,test}/**/*.{ts,js}'",
    "check:format": "prettier --check '{src,test}/**/*.{ts,js}'",
    "fix": "npm run fix:lint && npm run fix:format",
    "fix:lint": "eslint -c .eslintrc.fix.json --fix '{src,test}/**/*.{ts,js}'",
    "fix:format": "prettier --write '{src,test}/**/*.{ts,js}'",
    "dev": "npm run dev:watch",
    "dev:azblob": "concurrently -n azurite,app \"azurite-blob --silent --blobHost 0.0.0.0 --location ./data/azurite/blobs/\" \"npm run dev:watch\"",
    "dev:watch": "npm run clean && NODE_ENV=development LOG_LEVEL=debug tsc-watch --preserveWatchOutput -w --noEmitOnError false --onSuccess \"node -r module-alias/register -r source-map-support/register ./build/file-backend/src/app.js\"",
    "dev:debug": "npm run clean && NODE_ENV=development LOG_LEVEL=debug tsc-watch --preserveWatchOutput -w --noEmitOnError false --onSuccess \"node -r module-alias/register -r source-map-support/register --inspect-brk=0.0.0.0:9222 ./build/file-backend/src/app.js\"",
    "clean": "rm -rf build/",
    "compile": "npm run clean && echo Compiling... && tsc -p .",
    "compile:develop": "npm run compile",
    "compile:staging": "npm run compile:production",
    "compile:production": "npm run clean && echo Compiling... && tsc -p tsconfig.build.json",
    "precommit": "lint-staged"
  },
  "_moduleAliases": {
    "src": "build/file-backend/src/",
    "shared/common": "build/shared/common/src/",
    "shared/backend": "build/shared/backend/src/",
    "shared/nestjs": "build/shared/nestjs/src/"
  },
  "devDependencies": {
    "@elunic/ecs-prettier": "^0.2.1",
    "@elunic/ecs-tsconfig": "^0.1.1",
    "@elunic/eslint-config-ecs": "^0.1.12",
    "@types/concurrently": "^5.2.1",
    "@types/eslint": "^7.2.5",
    "@types/get-port": "^4.2.0",
    "@types/jest": "^25.2.3",
    "@types/mime": "^2.0.3",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^12.12.47",
    "@types/prettier": "^1.19.1",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sharp": "^0.30.0",
    "@types/sinon": "^7.5.2",
    "@types/source-map-support": "^0.5.1",
    "@types/supertest": "^2.0.9",
    "@types/tsc-watch": "^4.2.0",
    "azurite": "^3.8.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.14.0",
    "exifr": "^6.1.1",
    "get-port": "^5.1.1",
    "image-size": "^0.9.7",
    "jest": "^26.6.3",
    "jest-extended": "^1.2.0",
    "jest-html-reporters": "^2.1.0",
    "jest-junit": "^13.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.0.5",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~4.0.5"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.3.0",
    "file-type": "^16.1.0",
    "hasha": "^5.2.2",
    "istextorbinary": "^5.12.0",
    "mime": "^2.5.0",
    "module-alias": "^2.2.2",
    "node-cache": "^5.1.2",
    "read-chunk": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.30.7",
    "source-map-support": "^0.5.19"
  }
}
